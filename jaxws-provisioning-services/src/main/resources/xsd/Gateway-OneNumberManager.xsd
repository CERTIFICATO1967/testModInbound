<?xml version="1.0" encoding="UTF-8"?><schema xmlns="http://www.w3.org/2001/XMLSchema"	xmlns:commons="http://telecomitalia.it/SOA/TRCS/Gateway/Commons"	targetNamespace="http://telecomitalia.it/SOA/TRCS/Gateway"	elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:Q1="http://telecomitalia.it/SOA/TRCS/Gateway">	<import		namespace="http://telecomitalia.it/SOA/TRCS/Gateway/Commons"		schemaLocation="Gateway-Commons_v1.3.xsd" />	<element name="OneNumberManagerRequest">		<complexType>			<choice>				<element name="activate">					<complexType>						<complexContent>							<extension base="Q1:DeviceType"></extension>						</complexContent>					</complexType></element>				<element name="modify">					<complexType>						<complexContent>							<extension base="Q1:DeviceType">								<attribute name="oldImsiDevice"									type="commons:imsiType" use="required">								</attribute>							</extension>						</complexContent>					</complexType></element>				<element name="deactivate">					<complexType>						<complexContent>							<extension base="Q1:DeviceType"></extension>						</complexContent>					</complexType></element>			</choice>			<attribute name="phoneNumber" type="commons:PhoneNumber"				use="required">			</attribute>			<attribute name="subsystem" type="string" use="required"></attribute>			<attribute name="info" type="string"></attribute>			<attribute name="requestType"										use="optional" default="OneStep">										<simpleType>											<restriction												base="string">												<enumeration													value="OneStep">												</enumeration>												<enumeration													value="TwoStep">												</enumeration>											</restriction>										</simpleType>			</attribute>			<attribute name="noProv"								type="boolean" use="optional" default="false">			</attribute>     <attribute name="noNotify"								type="boolean" use="optional" default="false">			</attribute>		</complexType>	</element>	<element name="OneNumberManagerResponse"		type="commons:ResponseMessage" />	<complexType name="DeviceType">		<attribute name="phoneNumberDevice" type="commons:PhoneNumber"			use="required">		</attribute>		<attribute name="imsiDevice" type="commons:imsiType" use="required"></attribute>	</complexType></schema>