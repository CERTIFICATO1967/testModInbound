								<xs:attribute name="LimitDate" type="xs:dateTime" use="required"/>
								<xs:attribute name="BalanceInfo" type="xs:string" use="required"/>
								<xs:attribute name="EventList" use="required"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="EXTRADATA" minOccurs="0">
							<xs:annotation>
								<xs:documentation>ONLY FOR PREPAID</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence minOccurs="0">
									<xs:element name="DISCOUNTSTEPS" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Mandatory and valid only with CostType="SS"</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:choice maxOccurs="unbounded">
												<xs:element name="STEP" minOccurs="0" maxOccurs="4">
													<xs:complexType>
														<xs:attribute name="ID" use="required"/>
														<xs:attribute name="NEXTRENEWALS" use="required">
															<xs:annotation>
																<xs:documentation>The counter of the next extensions/renewals to do.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="COSTVALUE" use="required">
															<xs:annotation>
																<xs:documentation>The cost used in the extension or renewal batch operation.</xs:documentation>
															</xs:annotation>
															<xs:simpleType>
																<xs:restriction base="xs:decimal">
																	<xs:totalDigits value="10"/>
																	<xs:fractionDigits value="2"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:attribute>
														<xs:attribute name="USED" type="xs:boolean" use="required">
															<xs:annotation>
																<xs:documentation>Set to 'true' if the step is used to pay the extension/renew of the offer/accessory current period.
Otherwise, it is set to 'false'.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="MINNUMRENEWAL" use="required">
															<xs:annotation>
																<xs:documentation>INTERNAL USE
The Step if the offer.numextension or accessory.numrenewal is greater-equal than  step.minnumrenewal.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="MAXNUMRENEWAL" use="required">
															<xs:annotation>
																<xs:documentation>INTERNAL USE
The Step is valid until the offer.numextension or accessory.numrenewal is less-equal than  step.maxnumrenewal.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="COMPONENTSTATE" use="optional">
															<xs:annotation>
																<xs:documentation>INTERNAL USE</xs:documentation>
															</xs:annotation>
															<xs:simpleType>
																<xs:restriction base="xs:nonNegativeInteger"/>
															</xs:simpleType>
														</xs:attribute>
													</xs:complexType>
												</xs:element>
												<xs:element name="FINALSTEP">
													<xs:complexType>
														<xs:attribute name="ID" use="required"/>
														<xs:attribute name="COSTVALUE" use="required">
															<xs:annotation>
																<xs:documentation>The cost used in the extension or renewal batch operation.</xs:documentation>
															</xs:annotation>
															<xs:simpleType>
																<xs:restriction base="xs:decimal">
																	<xs:totalDigits value="10"/>
																	<xs:fractionDigits value="2"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:attribute>
														<xs:attribute name="USED" type="xs:boolean" use="required">
															<xs:annotation>
																<xs:documentation>Set to 'true' if the step is used to pay the extension/renew of the offer/accessory current period.
Otherwise, it is set to 'false'.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="MINNUMRENEWAL" use="required">
															<xs:annotation>
																<xs:documentation>INTERNAL USE
The Step if the offer.numextension or accessory.numrenewal is greater-equal than  step.minnumrenewal.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="COMPONENTSTATE" use="optional">
															<xs:annotation>
																<xs:documentation>INTERNAL USE</xs:documentation>
															</xs:annotation>
															<xs:simpleType>
																<xs:restriction base="xs:nonNegativeInteger"/>
															</xs:simpleType>
														</xs:attribute>
													</xs:complexType>
												</xs:element>
											</xs:choice>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="COMPONENTSTATE" use="optional">
									<xs:simpleType>
										<xs:restriction base="xs:nonNegativeInteger"/>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="OFFERID">
									<xs:simpleType>
										<xs:restriction base="xs:decimal">
											<xs:totalDigits value="8"/>
											<xs:fractionDigits value="0"/>
											<xs:minInclusive value="0"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="COSTTYPE" use="required">
									<xs:annotation>
										<xs:documentation>"DEF": Default
"CB" : Customer Base
"AL" : New account
"MNP": account for MNP
"PP" : Personal Price
"SS" : Discount Steps (Scala Sconti)</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="DEF"/>
											<xs:enumeration value="CB"/>
											<xs:enumeration value="AL"/>
											<xs:enumeration value="MNP"/>
											<xs:enumeration value="PP"/>
											<xs:enumeration value="SS"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="COSTVALUE" use="optional">
									<xs:annotation>
										<xs:documentation>Valued only for PersonalPrice (PP).</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:decimal">
											<xs:totalDigits value="10"/>
