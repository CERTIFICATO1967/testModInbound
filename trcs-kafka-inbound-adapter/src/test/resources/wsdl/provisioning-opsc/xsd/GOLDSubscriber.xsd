							<xs:complexType>
								<xs:attribute name="Name">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="10"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="MASTER" minOccurs="0">
							<xs:complexType>
								<xs:attribute name="Name">
									<xs:simpleType>
										<xs:restriction base="xs:string">
