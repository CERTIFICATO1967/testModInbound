	<xs:element name="OFFER">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="AccountComponentType">
					<xs:choice maxOccurs="unbounded">
						<xs:element ref="ACCESSORY" minOccurs="0" maxOccurs="unbounded"/>
