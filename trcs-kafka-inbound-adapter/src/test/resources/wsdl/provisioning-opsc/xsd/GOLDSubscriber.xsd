						<xs:annotation>
							<xs:documentation>This value is copied from the Offer model</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MODELCOST">
						<xs:simpleType>
							<xs:restriction base="xs:decimal">
								<xs:totalDigits value="21"/>
								<xs:fractionDigits value="6"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="MODELCOSTSTART" type="xs:dateTime"/>
					<xs:attribute name="MODELCOSTEND" type="xs:dateTime"/>
					<xs:attribute name="MODELCOSTFLAG" type="xs:integer"/>
					<xs:attribute name="MODELDISCOUNTVALUE">
						<xs:simpleType>
							<xs:restriction base="xs:decimal">
								<xs:totalDigits value="18"/>
								<xs:fractionDigits value="6"/>
							</xs:restriction>
						</xs:simpleType>
